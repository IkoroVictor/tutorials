<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/aop  http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">

    <context:annotation-config/>

    <bean id="vehicleWithSetterInjectionBean"
          class="com.baeldung.beaninjectiontypes.model.Vehicle">
        <property name="engine" ref="engineV8" />
    </bean>

    <bean id="vehicleWithConstructorInjectionBean"
          class="com.baeldung.beaninjectiontypes.model.Vehicle">
        <constructor-arg name="engine" ref="engineV8" />
    </bean>


    <bean id="engineV8" class="com.baeldung.beaninjectiontypes.model.engine.EngineImplV8" />

</beans>